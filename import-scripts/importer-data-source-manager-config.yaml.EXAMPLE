# database destination definitions (these must come third)
database_destinations:
  -
    database_destination_id: public
    configured_importer_host: knowledgesystems-importer
    all_recognized_source_repositories:
      - knowledge-systems-curated-studies
      - datahub 
      - private
      - impact
  -
    database_destination_id: genie
    configured_importer_host: knowledgesystems-importer
    all_recognized_source_repositories:
      - genie
  -
    database_destination_id: msk
    configured_importer_host: pipelines3
    all_recognized_source_repositories:
      - cmo-access
      - bic-mskcc-legacy
      - cmo-argos
      - private
      - impact
      - datahub_shahlab
      - msk-mind-datahub
      - knowledge-systems-curated-studies
      - datahub
      - pipelines-testing
      - extract-projects
  -
    database_destination_id: triage
    configured_importer_host: pipelines3
    all_recognized_source_repositories:
      - datahub
# end of file
